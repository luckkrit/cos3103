{
  "name": "slides_src",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "slidev --open",
    "export": "slidev export",
    "build_sql_select": "slidev build --base /cos3103/slides/sql_select/ --out ../public/slides/sql_select",
    "export_sql_select": "slidev export --with-clicks --format pptx --output ./slides-export/sql_select",
    "build_sql_storedproc_views": "slidev build --base /cos3103/slides/sql_storedproc_views/ --out ../public/slides/sql_storedproc_views",
    "export_sql_storedproc_views": "slidev export --with-clicks --format pptx --output ./slides-export/sql_storedproc_views"
  },
  "dependencies": {
    "@slidev/cli": "^52.1.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "playwright-chromium": "^1.54.1"
  }
}